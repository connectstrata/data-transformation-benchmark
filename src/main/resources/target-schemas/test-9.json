              "nullable": true,
              "required": [
                "fulfilled_orders"
              ],
              "properties": {
                "fulfilled_orders": {
                  "type": "array",
                  "minItems": 1,
                  "contains": {
                    "type": "object",
                    "required": [
                      "id"
                    ],
                    "properties": {
                      "id": {
                        "type": "number"
                      }
                    }
                  }
                }
              }
            }
          }
