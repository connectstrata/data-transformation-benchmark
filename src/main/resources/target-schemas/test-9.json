              "nullable": true
            },
            "last_name": {
              "type": "string",
              "description": "Individual's last name",
              "nullable": true
            },
            "organization": {
              "type": "string",
              "nullable": true
            },
            "locale": {
              "type": "string",
              "nullable": true
            },
            "title": {
              "type": "string",
              "nullable": true
            },
            "image": {
              "type": "string",
              "nullable": true
            },
            "location": {
              "type": "object",
              "properties": {
                "address1": {
                  "type": "string",
