            "profile"
          ]
        },
        "id": {
          "type": "string",
          "nullable": true
        },
        "attributes": {
          "type": "object",
          "required": [
            "email"
          ],
          "properties": {
            "email": {
              "type": "string",
              "description": "Individual's email address"
            },
            "phone_number": {
              "type": "string",
              "description": "Individual's phone number in E.164 format",
              "nullable": true
            },
            "external_id": {
              "type": "string",
              "nullable": true
            },
            "_kx": {
              "type": "string",
              "nullable": true
            },
            "first_name": {
              "type": "string",
