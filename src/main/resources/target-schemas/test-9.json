                  "type": "string",
                  "nullable": true
                },
                "city": {
                  "type": "string",
                  "nullable": true
                },
                "country": {
                  "type": "string",
                  "nullable": true
                },
                "region": {
                  "type": "string",
                  "nullable": true
                },
                "zip": {
                  "type": "string",
                  "nullable": true
                },
                "timezone": {
                  "type": "string",
                  "nullable": true
                },
                "ip": {
                  "type": "string",
                  "nullable": true
                }
              }
