{
  "type": "object",
  "description": "HubSpot CRM contact record from the Contacts API",
  "properties": {
    "id": {
      "type": "string",
      "description": "The unique HubSpot contact ID"
    },
    "url": {
      "type": "string",
      "description": "URL to view the contact in HubSpot"
    },
    "archived": {
      "type": "boolean",
      "description": "Whether the contact is archived"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp when the contact was created"
    },
    "updatedAt": {
      "type": "string",
      "format": "date-time",
      "description": "ISO 8601 timestamp when the contact was last updated"
    },
    "properties": {
      "type": "object",
      "description": "Contact properties (both standard and custom)",
      "properties": {
        "email": {
          "type": "string",
          "format": "email",
          "description": "The contact's email address"
        },
        "lastname": {
          "type": "string",
          "description": "The contact's last name"
        },
        "firstname": {
          "type": "string",
          "description": "The contact's first name"
        },
        "favoriteRestaurant1": {
          "type": "string",
          "description": "Custom property: first favorite restaurant"
        },
        "favoriteRestaurant2": {
          "type": "string",
          "description": "Custom property: second favorite restaurant"
        },
        "phoneNumber": {
          "type": "string",
          "description": "The contact's phone number"
        },
        "createdate": {
          "type": "string",
          "format": "date-time",
          "description": "ISO 8601 timestamp when the contact was created"
        },
        "hs_object_id": {
          "type": "string",
          "description": "The HubSpot object ID"
        },
        "lastmodifieddate": {
          "type": "string",
          "format": "date-time",
          "description": "ISO 8601 timestamp when the contact was last modified"
        }
      }
    }
  }
}
